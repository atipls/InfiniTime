{
   "lv_font_win95_clock": {
      "sources": [
         {
            "file": "fonts/sysfont.otf",
            "symbols": "0123456789.: -"
         }
      ],
      "bpp": 1,
      "size": 60,
      "format": "bin",
      "target_path": "/fonts/"
   },
   "lv_font_win95_normal": {
      "sources": [
         {
            "file": "fonts/WindowsRegular.ttf",
            "symbols": "0123456789.: -APMonTueWdhFriSat"
         }
      ],
      "bpp": 1,
      "size": 40,
      "format": "bin",
      "target_path": "/fonts/"
   },
   "lv_font_win95_small": {
      "sources": [
         {
            "file": "fonts/WindowsRegular.ttf",
            "range": "0x20-0xff"
         }
      ],
      "bpp": 1,
      "size": 12,
      "format": "bin",
      "target_path": "/fonts/"
   }
}
